{
  "manifest_version": 2,
  "name": "Github Confetti",
  "version": "1.1.0",
  "description": "Add confetti to your github issue checkboxes with Github Confetti",
  "author": "Alice Hendicott",
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "include_globs": ["*://git.*.com.au/*", "*://github.com/*"],
      "js": ["confetti.js"]
    }
  ]
}
